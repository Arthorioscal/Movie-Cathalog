<header> Movies </header>
        <div class="movies">
        <% @movies.each do |movie| %>
            <div class="movie">
                <h2><%= movie.title %></h2>
                <p>Release Year: <%= movie.year %></p>
                <p>Status: <%= movie.status %></p>
                <%= link_to "View Details", movie_path(movie) %>
            </div>
        <% end %>
        </div>
    </div>